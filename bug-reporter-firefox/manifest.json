{
  "manifest_version": 2,
  "name": "GenAlpaca Bug Reporter",
  "version": "1.0.0",
  "description": "Capture, annotate, and submit bug reports for GenAlpaca",
  "permissions": ["activeTab", "tabs", "storage", "https://aphrrfprbixmhissnjfn.supabase.co/*"],
  "browser_specific_settings": {
    "gecko": {
      "id": "bug-reporter@genalpaca.com",
      "strict_min_version": "126.0"
    },
    "gecko_android": {
      "strict_min_version": "126.0"
    }
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
